{
  "name": "@univ-lehavre/biblio-cli",
  "version": "0.0.2",
  "description": "La ligne de commande",
  "author": "Pierre-Olivier Chasset",
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/univ-lehavre/biblio.git",
    "directory": "packages/cli"
  },
  "bugs": {
    "url": "https://github.com/univ-lehavre/biblio/issues"
  },
  "scripts": {
    "build": "preconstruct build",
    "dev": "preconstruct watch",
    "clean": "rm -fR dist",
    "types:check": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.{ts,js,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,js,json,md}\"",
    "lint": "eslint \"src/**/*.{ts,js}\" --max-warnings=0",
    "lint:fix": "eslint \"src/**/*.{ts,js}\" --fix",
    "start:only": "node ./dist/univ-lehavre-biblio-cli.cjs.js",
    "start": "run-s build start:only"
  },
  "dependencies": {
    "@clack/prompts": "^1.0.0-alpha.4",
    "@univ-lehavre/biblio-validate-openalex": "workspace:^",
    "@univ-lehavre/biblio-openalex-types": "workspace:^",
    "@effect/experimental": "^0.56.0",
    "@effect/platform-node": "^0.98.0",
    "effect": "^3.17.13",
    "picocolors": "^1.1.1"
  },
  "devDependencies": {
    "@univ-lehavre/typescript-config": "^2.0.0",
    "npm-run-all": "^4.1.5"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com",
    "access": "public"
  },
  "files": [
    "dist",
    "CHANGELOG.md"
  ],
  "main": "dist/univ-lehavre-biblio-cli.cjs.js",
  "module": "dist/univ-lehavre-biblio-cli.esm.js",
  "exports": {
    ".": {
      "types": {
        "import": "./dist/univ-lehavre-biblio-cli.cjs.mjs",
        "default": "./dist/univ-lehavre-biblio-cli.cjs.js"
      },
      "module": "./dist/univ-lehavre-biblio-cli.esm.js",
      "import": "./dist/univ-lehavre-biblio-cli.cjs.mjs",
      "default": "./dist/univ-lehavre-biblio-cli.cjs.js"
    },
    "./package.json": "./package.json"
  },
  "babel": {
    "targets": {
      "node": "22.19"
    },
    "presets": [
      "@babel/preset-env"
    ],
    "overrides": [
      {
        "test": "**/*.ts",
        "presets": [
          "@babel/preset-typescript"
        ]
      }
    ]
  }
}
