{
  "name": "@univ-lehavre/biblio-openalex-types",
  "version": "3.0.0",
  "description": "Un ensemble d’outils pour le téléchargement de fichiers en TypeScript",
  "author": "Pierre-Olivier Chasset",
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/univ-lehavre/biblio.git",
    "directory": "packages/openalex-types"
  },
  "bugs": {
    "url": "https://github.com/univ-lehavre/biblio/issues"
  },
  "scripts": {
    "build": "preconstruct build",
    "dev": "preconstruct watch",
    "clean": "rm -fR dist && rm -fR .turbo && rm -fR node_modules",
    "types:check": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.{ts,js,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,js,json,md}\"",
    "lint": "eslint \"src/**/*.{ts,js}\" --max-warnings=0",
    "lint:fix": "eslint \"src/**/*.{ts,js}\" --fix"
  },
  "dependencies": {
    "@clack/prompts": "^1.0.0-alpha.4",
    "effect": "^3.17.13",
    "qs": "^6.14.0"
  },
  "devDependencies": {
    "@univ-lehavre/typescript-config": "^2.0.0"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com",
    "access": "public"
  },
  "files": [
    "dist",
    "CHANGELOG.md"
  ],
  "main": "dist/univ-lehavre-biblio-openalex-types.cjs.js",
  "module": "dist/univ-lehavre-biblio-openalex-types.esm.js",
  "exports": {
    ".": {
      "types": {
        "import": "./dist/univ-lehavre-biblio-openalex-types.cjs.mjs",
        "default": "./dist/univ-lehavre-biblio-openalex-types.cjs.js"
      },
      "module": "./dist/univ-lehavre-biblio-openalex-types.esm.js",
      "import": "./dist/univ-lehavre-biblio-openalex-types.cjs.mjs",
      "default": "./dist/univ-lehavre-biblio-openalex-types.cjs.js"
    },
    "./package.json": "./package.json"
  },
  "babel": {
    "targets": {
      "node": "22.19"
    },
    "presets": [
      "@babel/preset-env"
    ],
    "overrides": [
      {
        "test": "**/*.ts",
        "presets": [
          "@babel/preset-typescript"
        ]
      }
    ]
  }
}
